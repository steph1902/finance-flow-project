# GCP Environment Variables Template
# Copy this file to .env.gcp and fill in your actual values

# GCP Project Configuration
GCP_PROJECT_ID=your-project-id
GCP_REGION=asia-northeast1

# Database Configuration
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@/financeflow?host=/cloudsql/PROJECT:REGION:INSTANCE

# Authentication
JWT_SECRET=your-jwt-secret-here

# Google Gemini AI
GEMINI_API_KEY=your-gemini-api-key-here

# Redis Configuration
REDIS_URL=redis://your-redis-host:6379

# NextAuth Configuration (if using)
NEXTAUTH_URL=https://your-app.run.app
NEXTAUTH_SECRET=your-nextauth-secret

# Stripe (if using payment features)
STRIPE_SECRET_KEY=sk_live_your_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your_key

# Email Service (Resend)
RESEND_API_KEY=re_your_key

# Currency API
FIXER_API_KEY=your-fixer-api-key

# Plaid (if using bank integrations)
PLAID_CLIENT_ID=your-plaid-client-id
PLAID_SECRET=your-plaid-secret
PLAID_ENV=production
